<launch>
  <!-- Declare argument for selecting the node to run -->
  <arg name="run_node_A" default="false" />

  <!-- Conditionally launch safe_zone_vel_publisher.cpp or smart_turtle.cpp -->
  <node if="$(arg run_node_A)" pkg="your_package_name" type="safe_zone_vel_publisher" name="safe_zone_vel_publisher_node" output="screen" />

  <node unless="$(arg run_node_A)" pkg="your_package_name" type="smart_turtle" name="smart_turtle_node" output="screen" />
</launch>
